#include <stdio.h>
int main(){
    int a , b , c;
    printf("enter three number:");
    scanf("%d %d %d",&a , &b ,&c);
    if(a>b && a>c)
    printf("%d is greatest\n",a);
    else if(b>a && b>c)
    printf("%d is gratest\n",b);
    else if(c>a && c>b)
    printf("%d is greatest\n",c);
    else
    printf("enter different different number\n");
    return 0;
}